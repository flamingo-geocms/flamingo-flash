<?xml version="1.0" encoding="UTF-8"?>
<FLAMINGO xmlns:fmc="fmc" id="flamingo" lang="nl" languages="en,nl" tooltipdelay="300" commtojava="true" preloadtitle="Prototypemap" useexternalinterface="true">
	<style id=".tooltip" font-family="arial, sans-serif" font-size="11" color="#880000" display="block"/>
	<fmc:Container left="0" top="0" height="100%" width="100%">
		<fmc:MonitorLayer id="monitorl" left="37%" top="50%" width="50%" listento="map">
			<string id="waiting" en="Making... [layer] [percentage]%..." nl="Kaart aan het maken..."/>
			<string id="loading" en="Making... [layer] [percentage]%..." nl="Kaart aan het laden. [percentage]%"/>
		</fmc:MonitorLayer>
		<fmc:ToolGroup tool="zoom" listento="map" width="100%" height="100" left="10" top="10">
			<fmc:ToolZoomin id="zoom" skin="f2"/>
			<fmc:ToolZoomout left="30" skin="f2"/>
			<fmc:ToolPan left="60" skin="f2"/>
			<fmc:ToolMeasure left="90" skin="f2"/>
			<fmc:ToolIdentify left="120" skin="f2"/>
		</fmc:ToolGroup>
		<fmc:Coordinates listento="map" left="80%" top="95%" decimals="6">
			<style id=".xy" font-family="arial, sans-serif" font-size="11"/>
			<string id="xy" nl="X: [x]     Y: [y]"/>
		</fmc:Coordinates>
		<fmc:BorderNavigation listento="map" top="0%" width="100%" height="100%" skin="f1"/>
		<fmc:Map id="map" left="0" top="0%" height="100%" width="100%" bottom="bottom" visible="true" extent="12000,304000,280000,620000">
			<fmc:LayerIdentifyIcon id="identifyicon"/>
			<fmc:LayerOGWMS id="layer2" url="http://public-wms.kaartenbalie.nl/wms/nederland?Styles=" srs="EPSG:28992" query_layers="gemeenten_2006" layers="gemeenten_2006" showerrors="true" visible="true"/>
			<fmc:LayerOGWMS id="layer1" url="http://osm.kaartenbalie.nl/wms/mapserver-scale" srs="EPSG:28992" layers="#ALL#" showerrors="true">
				</fmc:LayerOGWMS>
		</fmc:Map>
		<fmc:Window visible="false" top="100" left="100" width="300" bottom="bottom" canresize="true" canclose="true" title="Identify results">
			<string id="tooltip_close" en="close" nl="sluiten"/>
			<style id=".title" font-family="verdana" font-size="13px" color="#666666" display="block" font-weight="normal"/>
			<style id=".titlefocus" font-family="verdana" font-size="13px" color="#666666" display="block" font-weight="bold"/>
			<fmc:IdentifyResultsHTML width="100%" height="100%" left="0" listento="map">
				<style id=".bold" font-family="verdana" font-size="18px" color="#333333" display="block" font-weight="bold"/>
				<style id=".normal" font-family="verdana" font-size="11px" color="#333333" display="block" font-weight="normal"/>
				<style id=".uitleg" font-family="verdana" font-size="11px" color="#0033cc" display="block" font-weight="normal" font-style="italic"/>
				<string id="layer2.gemeenten_2006" stripdatabase="true">
					<nl>
						<span class='normal'>
							<span class='bold'>Gemeente</span>
							<textformat tabstops='[20,150,100]'>
							\tGemeente naam\t[gm_naam]
							\tGemeente code\t[gm_code]
							\tAantal inwoners\t[aant_inw]
							</textformat>
						</span>
					</nl>
				</string>
			</fmc:IdentifyResultsHTML>
		</fmc:Window>
	</fmc:Container>
</FLAMINGO>
